{
	"compilerOptions": {
		"target": "es2021",
		"lib": [ "es2021" ],
		"module": "es2022",
		"moduleResolution": "node",
		"types": ["@cloudflare/workers-types"],
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"outDir": "dist",
		"declaration": true
	}
}